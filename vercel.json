{
    "version": 2,
    "name": "vercel-serverless-flask-api",
    "builds": [
        { "src": "*.py", "use": "@now/now-flask" , "config": { "maxLambdaSize": "30mb" } }
    ],
    "routes": [
        {
            "src": "/index", "dest": "/index.py"
        },
        {
            "src": "/bread", "dest": "/index.py"
        }
    ]
}